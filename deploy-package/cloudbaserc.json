{
    "envId": "offercome2025-9g14jitp22f4ddfc",
    "framework": {
        "name": "deploy-package",
        "plugins": {
            "function": {
                "use": "@cloudbase/framework-plugin-function",
                "inputs": {
                    "functionRootPath": "./server",
                    "functions": [
                        {
                            "name": "api-cases",
                            "config": {
                                "timeout": 30,
                                "runtime": "Nodejs16.13",
                                "memorySize": 256,
                                "entry": "api-cases/index.js",
                                "triggers": [
                                    {
                                        "name": "api-cases-http",
                                        "type": "http",
                                        "config": {
                                            "path": "/api/cases",
                                            "method": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
                                        }
                                    },
                                    {
                                        "name": "api-categories-http",
                                        "type": "http",
                                        "config": {
                                            "path": "/api/categories",
                                            "method": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            }
        }
    }
}
