{
    "envId": "offercome2025-9g14jitp22f4ddfc",
    "version": "2.0",
    "$schema": "https://framework-1258016615.tcloudbaseapp.com/schema/latest.json",
    "functionRoot": "./server",
    "functions": [
        {
            "name": "api",
            "runtime": "Nodejs16.13",
            "memorySize": 256,
            "timeout": 30,
            "entry": "index.main",
            "ignore": [
                "cloudbase-function.js",
                "tests/**",
                "logs/**"
            ]
        }
    ],
    "service": {
        "name": "offercome-api-v2",
        "path": "api-v2",
        "target": "api"
    },
    "framework": {
        "name": "project",
        "plugins": {
            "website": {
                "use": "@cloudbase/framework-plugin-website",
                "inputs": {
                    "buildCommand": "echo 'No build needed'",
                    "outputPath": "public",
                    "cloudPath": "/"
                }
            }
        }
    }
}
